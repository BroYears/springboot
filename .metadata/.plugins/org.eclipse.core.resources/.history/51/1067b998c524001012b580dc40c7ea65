package com.test.basic.dto;

public class User {

	private Long seq;
	private String name;
	private Integer age;
	private String address;
	private String gender;
	
	//중첩 클래스
	public static class Builder {
	
		private Long seq;
		private String name;
		private Integer age;
		private String address;
		private String gender;
		
		//setter
		public Builder setSeq(Long seq) {
			this.seq = seq;
			return this;
		}
		
	}
	
}









